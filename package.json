{
  "name": "scorm-xapi-wrapper",
  "version": "1.0.0",
  "type": "module",
  "description": "Framework-agnostic SCORM 2004 and xAPI wrapper - build custom e-learning courses with any web technology. Optimized for Cursor AI development.",
  "main": "dist/scorm-wrapper.cjs.js",
  "module": "dist/scorm-wrapper.esm.js",
  "browser": "dist/scorm-wrapper.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "templates",
    "tools",
    "cli.js",
    "README.md",
    "QUICKSTART.md",
    "CURSOR_AI_GUIDE.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm run watch"
  },
  "bin": {
    "create-scorm-course": "./cli.js",
    "package-scorm": "./tools/package-builder.js",
    "package-xapi": "./tools/package-xapi.js"
  },
  "keywords": [
    "scorm",
    "scorm2004",
    "scorm-1.2",
    "xapi",
    "tin-can",
    "elearning",
    "lms",
    "learning",
    "course-builder",
    "create-scorm-course",
    "e-learning",
    "training",
    "cursor-ai",
    "ai-assisted",
    "education",
    "moodle",
    "canvas",
    "blackboard"
  ],
  "author": "Nathan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/natenorm/scorm-xapi-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/natenorm/scorm-xapi-wrapper/issues"
  },
  "homepage": "https://github.com/natenorm/scorm-xapi-wrapper#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.9.6"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "commander": "^11.1.0"
  }
}

